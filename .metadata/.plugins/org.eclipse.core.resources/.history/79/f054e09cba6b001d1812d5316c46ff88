<script>
let title="";
let img="";
let img2="";

window.onload = ()=>{
	title = localStorage.getItem("title");
	titleDom.innerHTML = title;
	img = localStorage.getItem("img");
	imgDom.innerHTML = img;
	
	document.img.src = img;
}
</script>

<form name="f">
<table border="1">
<tr><th colspan="3"><span id="title"></span></th></tr>
