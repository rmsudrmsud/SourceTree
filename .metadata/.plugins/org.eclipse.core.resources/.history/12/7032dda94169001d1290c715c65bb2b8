function a(){
	let val = document.f.score.value;
	let msg ="합격";
	if(val<0 || val>100){
		alert("잘못된 점수");
		return; //함수종료
	}
	if(val<60){
		alert("불합격")
	}
	alert(val+":");
}

function b(){
	alert(f2.sel.value+"학년입니다");
}
</script>
