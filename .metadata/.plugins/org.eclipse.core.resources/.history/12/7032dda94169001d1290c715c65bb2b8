for(i=2;i<10;i++){
	document.write("<td>");
	document.write(i+"단 출력");
	while(dan <=9){

		i = 1; // 1~9

		while(i<=9){

			document.write(dan*i + " ");

			i++;

		}
		document.write();
		dan++;

	}  
	document.write("</td>");
}
