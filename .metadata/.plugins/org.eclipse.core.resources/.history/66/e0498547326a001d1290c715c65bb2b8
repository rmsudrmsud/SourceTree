<script>
let headImgs =["locket.jpg","head1.jpg"]
let cnt=0; //출력할 이미지 방번호

window.onload = () => {
	a(); //document.이름.속성! 
}

const a = () => {
	document.headImg.src = "../img/head/" + headimgs[cnt%2]; //document.이름.속성!
	cnt++; // 다음이미지로 넘김	
}
</script>
