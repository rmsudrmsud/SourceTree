<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script>
let title="";
let img="";

onload = () => {
	title = localStorage.getItem("title");
	img = localStorage.getImg("img");
	
	let titleDom = document.getElementById("title");
	titleDom.innerHTML = title;

	document.img.src = img;
}
</script>
</head>
<body>
<table border="1">
<tr><th colspan="3"><span id="title"></span></th></tr>
<tr><td rowspan="3"><img src="" name="img"></td>
</table>
</body>
</html>