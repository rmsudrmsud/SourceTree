<meta charset="UTF-8">
<title>Insert title here</title>
<script type = "text/javascript">
function add(x, y){
	return x+y;
}
//파라메터로 단수받아서 구구단 한 단 출력
function gob(i){
	for(j=1;j<10;j++){
		document.write(j*i);
	}
}
</script>
</head>
<body>
<script type="text/javascript">
let res = add(1,2);
document.write("res:"+ res+"<br/>");
res = add("aaa","bbb");
document.write("res:"+ res+"<br/>");
res = gob(2);
</script>
</body>
