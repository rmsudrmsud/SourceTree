<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
<script>
let title="";
let img="";
let img2="";

window.onload = ()=>{
	title = localStorage.getItem("title");
	img = localStorage.getItem("img");
	img2 = localStorage.getItem("img2");
	
	document.img.src = img;
}
</script>
</body>
</html>