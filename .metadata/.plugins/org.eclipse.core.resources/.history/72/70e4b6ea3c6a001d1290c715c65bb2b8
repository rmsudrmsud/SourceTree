<script>
let prods = [{"title":"컴퓨터", "price":2900000, "info":"내일도착보장", "img":"../img/computer.jpg"},
	{"title":"폰", "price":1000000, "info":"내일도착보장", "img":"../img/phone.jpg"},
	{"title":"tv", "price":2900000, "info":"내일도착보장", "img":"../img/tv.jpg"},
	{"title":"개", "price":290000, "info":"11월23일도착보장", "img":"../img/dog.jpg"},
	{"title":"개2", "price":290000, "info":"내일도착보장", "img":"../img/dog2.jpg"},
	{"title":"개3", "price":290000, "info":"내일도착보장", "img":"../img/dog3.jpg"}]
</script>
