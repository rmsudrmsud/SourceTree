<body>
<input type="button" value="1번상품" onclick="printObj(0)">
<input type="button" value="2번상품" onclick="printObj(1)">
<input type="button" value="3번상품" onclick="printObj(2)">
<input type="button" value="4번상품" onclick="printObj(3)">
<input type="button" value="5번상품" onclick="printObj(4)">
<input type="button" value="6번상품" onclick="printObj(5)">
<div id="res"></div>

<script>
// 파람으로 방번호 받아서 그 방에있는 객체의 정보를 출력하는 함수
const printObj = (idx) => {
	let obj = prods[idx];
	document.write("title:"+obj.title+"<br/>");
	document.write("price:"+obj.price+"<br/>");
	document.write("img:"+obj.img+"<br/>");
	document.write("info:"+obj.info+"<br/>");	
}

let obj = prods[0];
document.write("title:"+obj.title+"<br/>");
document.write("price:"+obj.price+"<br/>");
document.write("img:"+obj.img+"<br/>");
document.write("info:"+obj.info+"<br/>");

</script>
